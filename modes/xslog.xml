<?xml version="1.0"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<MODE>
    <RULES IGNORE_CASE="FALSE">
		<!-- good sip responses -->
		<EOL_SPAN_REGEXP HASH_CHAR="SIP/2.0" TYPE="FUNCTION">SIP/2.0\s[123]\d\d</EOL_SPAN_REGEXP>
		<!-- unexpected sip responses -->
		<EOL_SPAN_REGEXP HASH_CHAR="SIP/2.0" TYPE="INVALID">SIP/2.0\s[456]\d\d</EOL_SPAN_REGEXP>
		<SEQ_REGEXP AT_WORD_START="TRUE" HASH_CHAR="com" TYPE="KEYWORD3">com.+?Event</SEQ_REGEXP>
		<SEQ AT_WORD_START="TRUE" TYPE="INVALID">Exception</SEQ>
		<SEQ AT_WORD_START="TRUE" TYPE="INVALID">Warn</SEQ>
		<SEQ AT_WORD_START="TRUE" TYPE="INVALID">Notice</SEQ>
		
    	<KEYWORDS>
