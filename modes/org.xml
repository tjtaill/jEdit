<?xml version="1.0"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<MODE>
	<PROPS>
		<PROPERTY NAME="lineComment" VALUE="#" />
		<PROPERTY NAME="startComment" VALUE="#+BEGIN_COMMENT" />
		<PROPERTY NAME="endComment" VALUE="#+END_COMMENT" />
	</PROPS>
	
    <RULES IGNORE_CASE="FALSE">
