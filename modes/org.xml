		<EOL_SPAN TYPE="LITERAL3">****</EOL_SPAN>
		
		<EOL_SPAN TYPE="COMMENT1">*** TODO</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD2">*** DONE</EOL_SPAN>
		
		<EOL_SPAN TYPE="KEYWORD4">***</EOL_SPAN>

		<EOL_SPAN TYPE="COMMENT1">** TODO</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD2">** DONE</EOL_SPAN>
		
		<EOL_SPAN TYPE="LITERAL1">**</EOL_SPAN>

		<EOL_SPAN TYPE="COMMENT1">* TODO</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD2">* DONE</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD1">*</EOL_SPAN>
	
    </RULES>
</MODE>